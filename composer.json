{
    "name": "docomoinnovations/cloud_orchestrator",
    "description": "Cloud orchestrator distribution by Docomo Innovations",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Docomo Innovations",
            "homepage": "https://www.docomoinnovations.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "http://drupal.org/project/issues/cloud_orchestrator",
        "source": "http://cgit.drupalcode.org/cloud_orchestrator"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "dev": {
            "type": "github",
            "url": "https://github.com/docomoinnovations/cloud_orchestrator"
        }
    },
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "docroot"
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    },
    "require": {
        "hussainweb/drupal-composer-helper": "1.0.0-beta3",
        "composer/installers": "^1.0",
        "drupal/admin_toolbar": "^2.0",
        "drupal/module_filter": "^3.1",
        "drupal/cloud": "3.0.0-alpha1",
        "drupal/ldap": "^3.0@beta",
        "drupal/copyright_footer": "^1.3",
        "drupal/login_destination": "^1.0@alpha",
        "drupal/authorization": "^1.0@beta",
        "drupal/externalauth": "^1.1",
        "drupal/bootstrap": "^3.20",
        "drupal/cloud_orchestrator": "3.0.0-alpha1",
        "drupal/panels": "^4.4",
        "drupal/page_manager": "^4.0@beta",
        "drush/drush": "^8",
        "drupal/ctools": "^3.2"
    },
    "require-dev": {
        "drupal/console": "~1.0"
    }
}
